{
  "name": "non-fungible-ranker",
  "version": "0.1.0",
  "description": "Module for calculating the statistics of an nft collection",
  "main": "lib/index.js",
  "scripts": {
    "test": "tap 'test/**/**.test.js'",
    "lint": "eslint .",
    "lint:fix": "npm run lint -- --fix",
    "lint:staged": "lint-staged",
    "lint:commit": "commitlint --edit ",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Palaxx/nft-collection-stats.git"
  },
  "keywords": [
    "nft"
  ],
  "author": "Michele Palamidessi <michele.palamidessi@protonmail.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Palaxx/nft-collection-stats/issues"
  },
  "homepage": "https://github.com/Palaxx/nft-collection-stats#readme",
  "lint-staged": {
    "*/**/*.{js,jsx}": [
      "prettier --write",
      "eslint --fix"
    ]
  },
  "devDependencies": {
    "@commitlint/cli": "^16.2.3",
    "@commitlint/config-conventional": "^16.2.1",
    "console-log-checker": "^1.0.6",
    "eslint": "^8.25.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.0.1",
    "husky": "^8.0.1",
    "lint-staged": "12.3.4",
    "tap": "^16.3.0"
  },
  "tap": {
    "check-coverage": true,
    "branches": 80,
    "lines": 80,
    "functions": 80,
    "statements": 80
  }
}
